<%- contentFor('body') %>

<div class="site-wrapper-inner">

<div class="cover-container">

  <div class="masthead clearfix">
    <div class="inner">
      <h3 class="masthead-brand">NodeCMS</h3>

      <ul class="nav masthead-nav">
        
        <% for(var i = 0;i < model.topMenuItems.length; i++) { %>

          <li><a href="<%= model.topMenuItems[i].url %>"><%= model.topMenuItems[i].name %></a></li>
        <% } %>
        
      </ul>
    </div>
  </div>
	<div class="inner cover">
		<h1 class="cover-heading">Home template.</h1>
		<p><%- model.content %></p>
	</div>
  <div class="mastfoot">
    <div class="inner">
      <p>&copy; Company 2014</p>
    </div>
  </div>
</div>
</div>