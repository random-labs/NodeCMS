<h3><i class="fa fa-bell fa-fw"></i> <%= __('ADMIN_LOGS_LABEL_MAINHEADER') %></h3>

<div class='row'>
    <div class='col-lg-12'>
        <ol class='breadcrumb'>
            <li>
                <a href="/admin"><i class='fa fa-dashboard'></i> Home</a>                                              
            </li>
            <li>
                <i class='fa fa-bell'></i> Logs
            </li>
        </ol>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <td style="text-align: center"><%= __('ADMIN_LOGS_LABEL_TABLEHEADER_TYPE') %></td>
                        <td style="text-align: left"><%= __('ADMIN_LOGS_LABEL_TABLEHEADER_DATE')%></td>
                        <td style="text-align: left"><%= __('ADMIN_LOGS_LABEL_TABLEHEADER_USERNAME')%></td>
                        <td style="text-align: left"><%= __('ADMIN_LOGS_LABEL_TABLEHEADER_MESSAGE')%></td>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="logMessage in logs">
                        <td style="text-align: center; width: 50px;">
                            <span class="label" ng-class="logMessage.iconclass">{{ logMessage.icontext }}</span>
                        </td>
                        <td style="text-align: left; width: 175px">
                            <span>{{ logMessage.timestamp }}</span>
                        </td>
                        <td style="text-align: left; width: 150px">
                            <span>{{ logMessage.user }}</span>
                        </td>
                        <td style="text-align: left">
                            <span>{{ logMessage.message }}</span>
                        </td>
                    </tr>
                </tbody>
            </table>

        </div>
    </div>
</div>